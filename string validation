#include<stdio.h>
#include<string.h>
int main()
{
	char str[50];
	printf("Gopika.J\n192324032\n");
	printf("enter the string: ");
	scanf("%[\n]s",str);
	int len=strlen(str);
	if(str[0]='('&&str[len-1]!=')')
	{
		printf("\nThe string is not valid");
	}
	else
	printf("\nThe string is valid");
	return 0;
}
